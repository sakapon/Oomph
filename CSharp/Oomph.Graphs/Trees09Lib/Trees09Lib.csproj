    <AssemblyName>Oomph.Graphs.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>Oomph.Graphs.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <Title>Oomph.Graphs.$(MSBuildProjectName)</Title>
    <AssemblyTitle>Oomph.Graphs.$(MSBuildProjectName)</AssemblyTitle>
    <Product>Oomph</Product>
    <Description>A laboratory of trees.</Description>
    <Authors>Keiho Sakapon</Authors>
    <Copyright>Â© 2025 Keiho Sakapon</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
